# vim:filetype=sh
function _ES_set_loaded() {
  local funcname=$1
  [ -z "$funcname" ] && return
  echo "$EXPECTSSH_LOADED_FUNCS" | grep -q -w $funcname || {
    [ -n "$EXPECTSSH_LOADED_FUNCS" ] && EXPECTSSH_LOADED_FUNCS+=" "
    EXPECTSSH_LOADED_FUNCS+=$funcname
  }
  export EXPECTSSH_LOADED_FUNCS
}
export -f _ES_set_loaded
